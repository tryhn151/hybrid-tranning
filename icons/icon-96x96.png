<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 96 96" width="96" height="96">
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4A6C6F;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#6B9B9E;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background Circle -->
  <circle cx="48" cy="48" r="44" fill="#F8F7F4" stroke="#4A6C6F" stroke-width="2"/>
  
  <!-- Dumbbell Icon -->
  <g transform="translate(48,48)">
    <!-- Left Weight -->
    <rect x="-30" y="-10" width="8" height="20" rx="2" fill="url(#grad)"/>
    <rect x="-25" y="-8" width="4" height="16" rx="1" fill="#4A6C6F"/>
    
    <!-- Right Weight -->
    <rect x="22" y="-10" width="8" height="20" rx="2" fill="url(#grad)"/>
    <rect x="21" y="-8" width="4" height="16" rx="1" fill="#4A6C6F"/>
    
    <!-- Bar -->
    <rect x="-22" y="-1.5" width="44" height="3" rx="1.5" fill="#4A6C6F"/>
    
    <!-- Grip Lines -->
    <line x1="-12" y1="-4" x2="-12" y2="4" stroke="#6B9B9E" stroke-width="0.5"/>
    <line x1="-7" y1="-4" x2="-7" y2="4" stroke="#6B9B9E" stroke-width="0.5"/>
    <line x1="7" y1="-4" x2="7" y2="4" stroke="#6B9B9E" stroke-width="0.5"/>
    <line x1="12" y1="-4" x2="12" y2="4" stroke="#6B9B9E" stroke-width="0.5"/>
  </g>
  
  <!-- Small Text -->
  <text x="48" y="75" text-anchor="middle" font-family="Arial, sans-serif" font-size="7" font-weight="bold" fill="#4A6C6F">HYBRID</text>
</svg> 